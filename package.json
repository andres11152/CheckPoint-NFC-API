  "description": "Proyecto con Arquitectura Hexagonal, Node.js, Express y TypeScript con PostgreSQL y TypeORM.",
  "main": "dist/server.js",
  "scripts": {
  "clean": "rm -rf dist",
  "build": "npm install && babel src -d dist",
